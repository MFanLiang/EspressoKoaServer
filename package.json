{
  "name": "espresso-koa-server",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "description": "koa 后端服务框架，开箱即用的脚手架，由 koa-generator 提供的 Koa2 命令安装实现",
  "scripts": {
    "start": "nodemon bin/www",
    "start:dev": "cross-env NODE_ENV=development nodemon bin/www",
    "start:prod": "cross-env NODE_ENV=production nodemon bin/www",
    "prd": "cross-env NODE_ENV=production pm2 start bin/www --name=KoaServer --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "auto-models": "node ./config/sequelize-auto.config.js"
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@koa/cors": "^4.0.0",
    "@koa/multer": "^3.0.2",
    "axios": "^1.6.8",
    "canvas": "^2.11.2",
    "cross-env": "^7.0.3",
    "debug": "^4.1.1",
    "dotenv": "^16.4.5",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.7.0",
    "koa-body": "4.1.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "koa-websocket": "^7.0.0",
    "koa2-swagger-ui": "^5.10.0",
    "log4js": "^6.9.1",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.3",
    "mysql2": "^3.6.5",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.9.13",
    "pug": "^2.0.3",
    "qr-image": "^3.2.0",
    "require-directory": "^2.1.1",
    "sequelize": "^6.35.2",
    "sequelize-auto": "^0.8.8",
    "swagger-jsdoc": "^6.2.8",
    "uuid": "^9.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^1.19.1"
  },
  "_moduleAliases": {
    "root": ".",
    "@db": "./db",
    "@config": "./config/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MFanLiang/EspressoKoaServer.git"
  },
  "bugs": {
    "url": "https://github.com/MFanLiang/EspressoKoaServer/issues"
  },
  "homepage": "https://github.com/MFanLiang/EspressoKoaServer",
  "engines": {
    "node": ">=14",
    "mysql": ">=8.0"
  }
}
